{
  "npmScope": "pebula",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*",
    ".eslintrc.json": "*"
  },
  "projects": {
    "ngrid-demo-app": {
      "tags": ["type:app", "ngrid-demo-app"]
    },
    "ngrid-demo-app-e2e": {
      "tags": ["type:e2e-app", "parent:ngrid-demo-app"],
      "implicitDependencies": ["ngrid-demo-app"]
    },
    "ngrid": {
      "tags": ["name:ngrid", "type:lib"]
    },
    "ngrid-material": {
      "tags": ["name:material", "type:child-lib", "parent:ngrid"]
    }
  },
  "nx-enforce-module-boundaries": [
    true,
    {
      "allow": [],
      "depConstraints": [
        {
          "sourceTag": "name:ngrid",
          "onlyDependOnLibsWithTags": []
        },
        {
          "sourceTag": "parent:ngrid",
          "onlyDependOnLibsWithTags": [
            "name:ngrid",
            "parent:ngrid",
            "parent:material"
          ]
        },
        {
          "sourceTag": "*",
          "onlyDependOnLibsWithTags": ["*"]
        }
      ]
    }
  ],
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
